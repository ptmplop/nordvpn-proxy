# Make a new build
docker build -f Dockerfile -t ptmplop/nordvpn-proxy:latest .

# Push to repository
docker push ptmplop/nordvpn-proxy:latest

# Run the container
docker run -it --rm --cap-add NET_ADMIN -p 1080:1080 -e USER=paul@morris.so -e PASS='silverbook' -e COUNTRY=Singapore ptmplop/nordvpn-proxy:latest
